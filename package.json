{
    "name": "mrsmiley",
    "displayName": "MrSmiley",
    "description": "A simple game in a VSCode extension",
    "version": "0.0.1",
    "publisher": "bbaker13",
    "engines": {
        "vscode": "^1.17.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.mrSmiley"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [{
            "command": "extension.mrSmiley",
            "title": "Adventures of Mr. Smiley"
        }]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "repository": "git://github.com/bbaker13/mrSmiley",
    "devDependencies": {
        "typescript": "^2.5.2",
        "vscode": "^1.1.5",
        "mocha": "^3.5.0",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.0",
        "@types/mocha": "^2.2.42"
    }
}